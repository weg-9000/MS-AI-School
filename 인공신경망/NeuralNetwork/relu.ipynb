        "# coding: utf-8\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "def relu(x):\n",
        "    return np.maximum(0,x)\n",
        "\n",
        "X = np.arange(-5.0,5.0,0.1)\n",
        "Y = relu(X)\n",
        "plt.plot(X,Y)\n",
        "plt.ylim(-0.1,5.0)\n",
        "plt.show()\n"
      ]
    }
